<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>CheckMemoryServlet</display-name>
	
	<servlet>
    		<servlet-name>CheckMemoryServlet</servlet-name>
    		<servlet-class>gengfo.mypack.servlet.HelloWorldWeblogicServlet</servlet-class>
    		<load-on-startup>1</load-on-startup> 
  	</servlet>
  	
  	<servlet-mapping>
    		<servlet-name>CheckMemoryServlet</servlet-name>
    		<url-pattern>UP_CheckMemoryServlet</url-pattern>
  	</servlet-mapping>  
  	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
</web-app>